@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";

}

<section class="content-header">
    <h1>
        Entry Page
        <small>Control panel</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">Dashboard</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <!-- Main row -->
    <div class="row">
        <!-- Left col -->
        <section class="col-lg-12">
            <!-- Horizontal Form -->
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Exam Entry Form</h3>
                </div>
                <!-- /.box-header -->
                <!-- form start -->
                <form class="form-horizontal">
                    <div class="box-body">
                       
                        <div class="form-group">
                            <label for="inputEmail1" class="col-sm-3 control-label">Organization</label>

                            <div class="col-sm-7">
                                <select class="form-control">
                                    <option value="1">Option1</option>
                                    <option value="1">Option1</option>
                                    <option value="1">Option1</option>
                                    <option value="1">Option1</option>
                                    <option value="1">Option1</option>
                                    <option value="1">Option1</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputEmail2" class="col-sm-3 control-label">Course</label>

                            <div class="col-sm-7">
                                <select class="form-control">
                                    <option value="1">Option1</option>
                                    <option value="1">Option1</option>
                                    <option value="1">Option1</option>
                                    <option value="1">Option1</option>
                                    <option value="1">Option1</option>
                                    <option value="1">Option1</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputEmail1" class="col-sm-3 control-label">Exam Type</label>

                            <div class="col-sm-7">
                                <select class="form-control">
                                    <option value="1">Option1</option>
                                    <option value="1">Option1</option>
                                    <option value="1">Option1</option>
                                    <option value="1">Option1</option>
                                    <option value="1">Option1</option>
                                    <option value="1">Option1</option>
                                </select>
                            </div>
                        </div>

                        <div class="form-group">
                            <label for="inputPassword1" class="col-sm-3 control-label">Code</label>

                            <div class="col-sm-4">
                                <input type="password" class="form-control"  placeholder="Code">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputPassword3" class="col-sm-3 control-label">Topic</label>

                            <div class="col-sm-7">
                                <input type="password" class="form-control"  placeholder="Topic">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="inputPassword4" class="col-sm-3 control-label">Full Marks</label>

                            <div class="col-sm-4">
                                <input type="password" class="form-control"  placeholder="Full Marks">
                            </div>
                        </div>
                      
                        <div class="form-group">
                            <label for="inputPassword4" class="col-sm-3 control-label">Duration</label>

                            
                        </div>
                   
                    </div>
                    <!-- /.box-body -->
                    <div class="box-footer">
                        <button type="submit" class="btn btn-info pull-right">Save</button>
                    </div>
                    <!-- /.box-footer -->
                </form>
            </div>
        </section>
        <!-- /.Main Board -->
        <!-- right col -->
    </div>
    <!-- /.row (main row) -->
</section>

